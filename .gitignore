# =============================================================================

.vscode/*
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json
*.code-workspace

# =============================================================================
# JetBrains Rider
# =============================================================================

.idea/
*.sln.iml

# =============================================================================
# Secrets and Sensitive Files
# =============================================================================

# Environment-specific settings (keep templates, ignore local overrides)
appsettings.*.local.json
appsettings.Local.json
local.settings.json

# Certificates and keys
*.pfx
*.p12
*.key
*.pem
*.crt
*.cer

# Secrets
secrets.json
secrets.*.json

# Azure
.azure/
serviceconfig.*.cscfg

# AWS
.aws/

